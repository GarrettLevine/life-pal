{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "small tiny react starter",
  "main": "webpack.config.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bulma": "^0.6.1",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-babel-webpack-plugin": "^1.1.0",
    "flow-bin": "^0.59.0",
    "flow-remove-types": "^1.2.3",
    "flowtype-loader": "^0.2.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "prop-types-extra": "^1.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "scripts": {
    "start": "npm run clean && npm run watch",
    "watch": "./node_modules/.bin/npm-run-all --parallel watch-backend watch-frontend",
    "clean": "rm -rf public && mkdir -p public",
    "watch-frontend": "npm run && NODE_ENV='development' ./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js --inline",
    "watch-backend": "npm run clean-backend && NODE_ENV='development' NODE_PATH=`pwd`/lib ./node_modules/.bin/nodemon ./lib/server.js",
    "clean-backend": "./node_modules/.bin/flow-remove-types ./src/lib/ -d ./lib"
  },
  "author": "",
  "license": "ISC"
}
